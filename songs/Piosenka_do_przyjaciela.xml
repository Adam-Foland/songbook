<?xml version="1.0" encoding="utf-8"?>
<song xmlns="http://21wdh.staszic.waw.pl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://21wdh.staszic.waw.pl Song.xsd" title="Piosenka do przyjaciela">
  <alias/>
  <text_author>Wojciech Bellon</text_author>
  <comment/>
  <composer type="solo"/>
  <artist type="solo"/>
  <translator/>
  <original_title/>
  <lyric>
    <block type="verse">
      <row important_over="false">Przeczeka jeszcze jedną zimę</row>
      <row important_over="false">Żeby znów móc się spotkać przyjacielu</row>
      <row important_over="false">Opowiedzieć coś dobrego</row>
      <row important_over="false">I zapomnieć o tych złych chwilach</row>
    </block>
    <block type="verse">
      <row important_over="false">Przeczekamy a gdy wiosną wyschną drogi</row>
      <row important_over="false">Dogonimy tych co przeszli obok</row>
      <row important_over="false">Przeczekamy jeszcze jedno lato</row>
      <row important_over="false">Wciąż nie wierząc że na karku mamy jesień</row>
    </block>
    <block type="verse">
      <row important_over="false">A dziewczynom co odeszły</row>
      <row important_over="false">Odeślemy list z deszczu</row>
      <row important_over="false">Przeczekamy żeby życia zbyt nie trudzić</row>
      <row important_over="false">A epilog dopiszemy garścią złudzeń</row>
    </block>
    <block type="verse">
      <row important_over="false">Przeczekamy ciszę między nami</row>
      <row important_over="false">Płoche słowa i rozmowy nieskończone</row>
      <row important_over="false">Chleba nigdy nam nie zabrakło</row>
      <row important_over="false">Popatrz, znowu kwitnie jabłoń</row>
      <row important_over="false">Przyjacielu gwiazda spadła</row>
    </block>
  </lyric>
  <music metre="">
    <guitar barre="0"/>
  </music>
  <status>
    <verificators/>
    <to_do/>
  </status>
</song>
